{
  "$schema": "http://json.schemastore.org/tsconfig",
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "outDir": "../../dist/lib/storybook-hubert",
    "types": [
      "node",
      "vite/client"
    ],
  },
  "exclude": [
    "**/*.test.ts",
    "**/*.test.tsx"
  ],
  "include": [
    "src/**/*.ts",
    "src/**/*.ts?",
    ".storybook/**/*.ts",
    ".storybook/**/*.ts?"
  ]
}
