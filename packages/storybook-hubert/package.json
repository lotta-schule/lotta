{
  "$schema": "https://raw.githubusercontent.com/SchemaStore/schemastore/master/src/schemas/json/package.json",
  "name": "@lotta-schule/storybook-hubert",
  "version": "4.2.0-alpha.8",
  "description": "Storybook Documentation for Hubert",
  "author": "Alexis Rinaldoni <alexis.rinaldoni@einsa.net>",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "dev": "storybook dev -p 6006",
    "e2e": "concurrently -k -s 'first' 'npx http-server test-sb-static --port 6006 --silent' 'wait-on tcp:6006 && npx test-storybook --ci --coverage --coverageDirectory ./coverage'",
    "e2e:prepare": "NODE_ENV=test npm run build -- --output-dir=test-sb-static",
    "lint": "eslint --ext js,jsx,ts,tsx src",
    "typecheck": "tsc --noEmit",
    "build": "storybook build"
  },
  "type": "module",
  "source": "src/index.ts",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@lotta-schule/hubert": "4.2.0-alpha.8"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
    "@lotta-schule/storybook-addon-theme": "4.2.0-alpha.8",
    "@storybook/addon-a11y": "^7.2.3",
    "@storybook/addon-actions": "^7.2.3",
    "@storybook/addon-essentials": "^7.2.3",
    "@storybook/addon-interactions": "^7.2.3",
    "@storybook/addon-links": "^7.2.3",
    "@storybook/addon-mdx-gfm": "^7.2.3",
    "@storybook/blocks": "^7.2.3",
    "@storybook/jest": "^0.2.2",
    "@storybook/react": "^7.2.3",
    "@storybook/react-webpack5": "^7.2.3",
    "@storybook/test-runner": "^0.13.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.2.20",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.0",
    "css-loader": "^6.8.1",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-storybook": "^0.6.13",
    "http-server": "^14.1.1",
    "identity-obj-proxy": "^3.0.0",
    "prettier": "^3.0.1",
    "storybook": "^7.2.3",
    "style-loader": "^3.3.3",
    "wait-on": "^7.0.1",
    "webpack": "^5.88.2",
    "whatwg-fetch": "^3.6.17"
  },
  "nx": {
    "$schema": "https://raw.githubusercontent.com/nrwl/nx/master/packages/nx/schemas/project-schema.json",
    "namedInputs": {
      "default": [
        "{projectRoot}/**/*"
      ]
    },
    "targets": {
      "dev": {
        "inputs": [
          "default"
        ],
        "dependsOn": [
          "^build"
        ]
      },
      "build": {
        "outputs": [
          "{projectRoot}/storybook-static"
        ],
        "inputs": [
          "default"
        ],
        "dependsOn": [
          "^build"
        ]
      },
      "e2e": {
        "outputs": [
          "{projectRoot}/coverage"
        ],
        "inputs": [
          "{projectRoot}/test-sb-static"
        ],
        "dependsOn": [
          "e2e:prepare"
        ]
      },
      "e2e:prepare": {
        "inputs": [
          "default"
        ],
        "outputs": [
          "{projectRoot}/test-sb-static"
        ]
      }
    }
  }
}
