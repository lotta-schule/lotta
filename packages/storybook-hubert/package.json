{
  "$schema": "https://raw.githubusercontent.com/SchemaStore/schemastore/master/src/schemas/json/package.json",
  "name": "@lotta-schule/storybook-hubert",
  "version": "4.2.1-alpha.0",
  "description": "Storybook Documentation for Hubert",
  "author": "Alexis Rinaldoni <alexis.rinaldoni@einsa.net>",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "dev": "storybook dev -p 6006",
    "e2e": "concurrently -k -s 'last' 'http-server test-sb-static --port 6006 --silent' -P 'wait-on tcp:6006 && test-storybook {@}'",
    "e2e:prepare": "NODE_ENV=test nx build storybook-hubert --test --output-dir=test-sb-static",
    "lint": "eslint --ext js,jsx,ts,tsx src",
    "typecheck": "tsc --noEmit",
    "build": "storybook build"
  },
  "type": "module",
  "devDependencies": {
    "@lotta-schule/storybook-addon-theme": "4.2.1-alpha.0",
    "@storybook/addon-a11y": "^8.0.6",
    "@storybook/addon-actions": "^8.0.6",
    "@storybook/addon-coverage": "^1.0.1",
    "@storybook/addon-essentials": "^8.0.6",
    "@storybook/addon-interactions": "^8.0.6",
    "@storybook/addon-links": "^8.0.6",
    "@storybook/addon-mdx-gfm": "^8.0.6",
    "@storybook/addon-storysource": "^8.0.6",
    "@storybook/addon-themes": "^8.0.6",
    "@storybook/addon-viewport": "^8.0.6",
    "@storybook/blocks": "^8.0.6",
    "@storybook/preview-api": "^8.0.6",
    "@storybook/react": "^8.0.6",
    "@storybook/react-vite": "^8.0.6",
    "@storybook/test": "^8.0.6",
    "@storybook/test-runner": "^0.17.0",
    "chromatic": "^11.2.0",
    "concurrently": "^8.2.2",
    "css-loader": "^6.10.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-storybook": "^0.8.0",
    "framer-motion": "^11.0.20",
    "http-server": "^14.1.1",
    "identity-obj-proxy": "^3.0.0",
    "prettier": "^3.2.5",
    "storybook": "^8.0.6",
    "style-loader": "^3.3.4",
    "wait-on": "^7.2.0",
    "whatwg-fetch": "^3.6.20"
  },
  "nx": {
    "$schema": "https://raw.githubusercontent.com/nrwl/nx/master/packages/nx/schemas/project-schema.json",
    "namedInputs": {
      "default": [
        "{projectRoot}/**/*"
      ]
    },
    "targets": {
      "dev": {
        "inputs": [
          "default"
        ],
        "dependsOn": [
          "^build"
        ]
      },
      "build": {
        "outputs": [
          "{projectRoot}/storybook-static"
        ],
        "inputs": [
          "default"
        ],
        "dependsOn": [
          "^build"
        ]
      },
      "e2e": {
        "outputs": [
          "{projectRoot}/coverage"
        ],

        "inputs": [
          "{projectRoot}/test-sb-static"
        ],
        "dependsOn": [
          "e2e:prepare"
        ]
      },
      "e2e:prepare": {
        "inputs": [
          "default"
        ],
        "outputs": [
          "{projectRoot}/test-sb-static"
        ]
      }
    }
}
}
