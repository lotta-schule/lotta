{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "storybook-hubert",
  "namedInputs": {
    "default": ["{projectRoot}/**/*"]
  },
  "targets": {
    "dev": {
      "inputs": ["default"],
      "dependsOn": ["^build"]
    },
    "build": {
      "inputs": ["default"],
      "outputs": ["{projectRoot}/storybook-static"],
      "dependsOn": ["^build"]
    },
    "e2e": {
      "outputs": ["{projectRoot}/coverage"],
      "inputs": ["{projectRoot}/test-sb-static"],
      "dependsOn": ["e2e:prepare"]
    },
    "e2e:prepare": {
      "inputs": ["default"],
      "outputs": ["{projectRoot}/test-sb-static"]
    }
  }
}
