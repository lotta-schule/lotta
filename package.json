    "scripts": {
        "start": "NODE_OPTIONS='--inspect' next dev",
        "build": "next build",
        "start:prod": "node server.js",
        "test": "jest",
        "test:ci": "jest --reporters=jest-junit --reporters=default --ci --testResultsProcessor='jest-junit' --collectCoverageFrom='src/**/*.{ts,tsx}' --collectCoverageFrom='!src/**/*.stories.{ts,tsx}'",
