{
  "name": "web",
  "version": "2.1.3",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --roots \"${PWD}/src\" --env=jest-environment-jsdom-sixteen --reporters=default",
    "test:ci": "react-scripts test --roots \"${PWD}/src\" --env=jest-environment-jsdom-sixteen --reporters=jest-silent-reporter",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src .",
    "lint:fix": "eslint --ext .ts,.tsx,.js,.jsx src . --fix",
    "apollo:schema:download": "apollo client:download-schema src/api/schema.json",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">1%",
      "not dead",
      "safari >= 11.1",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
