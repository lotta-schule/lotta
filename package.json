{
  "name": "web",
  "version": "2.3.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --roots \"${PWD}/src\" --env=jest-environment-jsdom-sixteen --reporters=default",
    "test:ci": "react-scripts test --roots \"${PWD}/src\" --env=jest-environment-jsdom-sixteen --reporters=jest-silent-reporter",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src .",
    "apollo:schema:download": "apollo client:download-schema src/api/schema.json",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "moduleNameMapper": {
      "react-spring/renderprops": "<rootDir>/node_modules/react-spring/renderprops.cjs.js",
      "react-spring": "<rootDir>/node_modules/react-spring/web.cjs.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">1%",
      "not dead",
      "safari >= 11.1",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@absinthe/socket": "^0.2.1",
    "@absinthe/socket-apollo-link": "^0.2.1",
    "@apollo/client": "^3.3.7",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@date-io/date-fns": "^1.3.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.12",
    "@jest/globals": "^26.6.2",
    "@jumpn/utils-graphql": "^0.6.0",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.51",
    "@material-ui/pickers": "^3.2.10",
    "@sentry/react": "^6.0.3",
    "apollo-v3-absinthe-upload-link": "^1.5.0",
    "axios": "^0.21.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.16.1",
    "file-saver": "^2.0.5",
    "history": "^5.0.0",
    "i18next": "^19.8.5",
    "is-url": "^1.2.4",
    "lodash": "^4.17.20",
    "material-ui-popup-state": "^1.7.1",
    "matomo-ts": "^1.0.1",
    "phoenix": "^1.5.7",
    "react": "^16.13.1",
    "react-app-polyfill": "^2.0.0",
    "react-beautiful-dnd": "^13.0.0",
    "react-cloudimage-responsive": "^1.6.10",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.2.4",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.8.5",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "react-spring": "^8.0.27",
    "react-swipeable-views": "^0.13.9",
    "react-virtualized": "^9.22.3",
